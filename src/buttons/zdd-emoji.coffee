  renderMenu: ->
    str = __EMOJI.map (i)->
      return """<li><a href="javascript:;" class="font-color">#{i}</a></li>"""
    .join ''
    str = """
      <ul class="zdd-emoji-list">
        #{str}
      </ul>
    """
    @menuWrapper[0].innerHTML = str;

    @menuWrapper.on 'mousedown', '.zdd-emoji-list', (e) ->
      false
    @menuWrapper.on 'click', 'a', (e) =>
      @wrapper.removeClass('menu-on')
      range = @editor.selection.range()
      textNode = document.createTextNode($(e.currentTarget).text())

      range.insertNode textNode
      @editor.trigger 'valuechanged'
Simditor.Toolbar.addButton ZddEmojiButton
